@import models.subscription.{upeJourney, fmJourney, groupJourney, contactJourney}
@import models.grs.EntityType
@import utils.countryOptions.CountryOptions

@this()

@(upeModel: upeJourney, fmModel: fmJourney, groupModel: groupJourney, contactModel: contactJourney, countryOptions: CountryOptions)(implicit request: RequestHeader, messages: Messages)

<fo:root xml:lang="@messages.lang.locale.getLanguage" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:fox="http://xmlgraphics.apache.org/fop/extensions" font-family="sans-serif">

    <fo:layout-master-set>
        <fo:simple-page-master master-name="main" page-height="297mm" page-width="210mm" margin="2cm">
            <fo:region-body region-name="xsl-region-body" margin-top="3cm"/>
            <fo:region-before region-name="xsl-region-before"/>
            <fo:region-after region-name="xsl-region-after"/>
        </fo:simple-page-master>
    </fo:layout-master-set>

    <fo:declarations>
        <pdf:catalog xmlns:pdf="http://xmlgraphics.apache.org/fop/extensions/pdf">
            <pdf:dictionary type="normal" key="ViewerPreferences">
                <pdf:boolean key="DisplayDocTitle">true</pdf:boolean>
            </pdf:dictionary>
        </pdf:catalog>
    </fo:declarations>

    <fo:bookmark-tree>
        <fo:bookmark internal-destination="title">
            <fo:bookmark-title>@messages("service.name")</fo:bookmark-title>

            <fo:bookmark internal-destination="filing-member-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.filingmember")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="primary-contact-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.first")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="secondary-contact-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.second")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="contact-address-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.address")</fo:bookmark-title>
            </fo:bookmark>

        </fo:bookmark>
    </fo:bookmark-tree>

    <fo:page-sequence master-reference="main">

        <!-- Header -->
        <fo:static-content flow-name="xsl-region-before">
            <fo:block border-color="#1d70b8" border-after-style="solid" border-after-width="5pt">
                <fo:block text-align="left">
                    <fo:instream-foreign-object>
                        <svg
                                focusable="false"
                                role="img"
                                class="govuk-header__logotype"
                                xmlns="http://www.w3.org/2000/svg"
                                viewBox="0 0 148 30"
                                height="20"
                                aria-label="GOV.UK">
                            <title>GOV.UK</title>
                            <path d="M22.6 10.4c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4m-5.9 6.7c-.9.4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4m10.8-3.7c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s0 2-1 2.4m3.3 4.8c-1 .4-2-.1-2.4-1-.4-.9.1-2 1-2.4.9-.4 2 .1 2.4 1s-.1 2-1 2.4M17 4.7l2.3 1.2V2.5l-2.3.7-.2-.2.9-3h-3.4l.9 3-.2.2c-.1.1-2.3-.7-2.3-.7v3.4L15 4.7c.1.1.1.2.2.2l-1.3 4c-.1.2-.1.4-.1.6 0 1.1.8 2 1.9 2.2h.7c1-.2 1.9-1.1 1.9-2.1 0-.2 0-.4-.1-.6l-1.3-4c-.1-.2 0-.2.1-.3m-7.6 5.7c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s0 2 1 2.4m-5 3c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s.1 2 1 2.4m-3.2 4.8c.9.4 2-.1 2.4-1 .4-.9-.1-2-1-2.4-.9-.4-2 .1-2.4 1s0 2 1 2.4m14.8 11c4.4 0 8.6.3 12.3.8 1.1-4.5 2.4-7 3.7-8.8l-2.5-.9c.2 1.3.3 1.9 0 2.7-.4-.4-.8-1.1-1.1-2.3l-1.2 4c.7-.5 1.3-.8 2-.9-1.1 2.5-2.6 3.1-3.5 3-1.1-.2-1.7-1.2-1.5-2.1.3-1.2 1.5-1.5 2.1-.1 1.1-2.3-.8-3-2-2.3 1.9-1.9 2.1-3.5.6-5.6-2.1 1.6-2.1 3.2-1.2 5.5-1.2-1.4-3.2-.6-2.5 1.6.9-1.4 2.1-.5 1.9.8-.2 1.1-1.7 2.1-3.5 1.9-2.7-.2-2.9-2.1-2.9-3.6.7-.1 1.9.5 2.9 1.9l.4-4.3c-1.1 1.1-2.1 1.4-3.2 1.4.4-1.2 2.1-3 2.1-3h-5.4s1.7 1.9 2.1 3c-1.1 0-2.1-.2-3.2-1.4l.4 4.3c1-1.4 2.2-2 2.9-1.9-.1 1.5-.2 3.4-2.9 3.6-1.9.2-3.4-.8-3.5-1.9-.2-1.3 1-2.2 1.9-.8.7-2.3-1.2-3-2.5-1.6.9-2.2.9-3.9-1.2-5.5-1.5 2-1.3 3.7.6 5.6-1.2-.7-3.1 0-2 2.3.6-1.4 1.8-1.1 2.1.1.2.9-.3 1.9-1.5 2.1-.9.2-2.4-.5-3.5-3 .6 0 1.2.3 2 .9l-1.2-4c-.3 1.1-.7 1.9-1.1 2.3-.3-.8-.2-1.4 0-2.7l-2.9.9C1.3 23 2.6 25.5 3.7 30c3.7-.5 7.9-.8 12.3-.8m28.3-11.6c0 .9.1 1.7.3 2.5.2.8.6 1.5 1 2.2.5.6 1 1.1 1.7 1.5.7.4 1.5.6 2.5.6.9 0 1.7-.1 2.3-.4s1.1-.7 1.5-1.1c.4-.4.6-.9.8-1.5.1-.5.2-1 .2-1.5v-.2h-5.3v-3.2h9.4V28H55v-2.5c-.3.4-.6.8-1 1.1-.4.3-.8.6-1.3.9-.5.2-1 .4-1.6.6s-1.2.2-1.8.2c-1.5 0-2.9-.3-4-.8-1.2-.6-2.2-1.3-3-2.3-.8-1-1.4-2.1-1.8-3.4-.3-1.4-.5-2.8-.5-4.3s.2-2.9.7-4.2c.5-1.3 1.1-2.4 2-3.4.9-1 1.9-1.7 3.1-2.3 1.2-.6 2.6-.8 4.1-.8 1 0 1.9.1 2.8.3.9.2 1.7.6 2.4 1s1.4.9 1.9 1.5c.6.6 1 1.3 1.4 2l-3.7 2.1c-.2-.4-.5-.9-.8-1.2-.3-.4-.6-.7-1-1-.4-.3-.8-.5-1.3-.7-.5-.2-1.1-.2-1.7-.2-1 0-1.8.2-2.5.6-.7.4-1.3.9-1.7 1.5-.5.6-.8 1.4-1 2.2-.3.8-.4 1.9-.4 2.7zM71.5 6.8c1.5 0 2.9.3 4.2.8 1.2.6 2.3 1.3 3.1 2.3.9 1 1.5 2.1 2 3.4s.7 2.7.7 4.2-.2 2.9-.7 4.2c-.4 1.3-1.1 2.4-2 3.4-.9 1-1.9 1.7-3.1 2.3-1.2.6-2.6.8-4.2.8s-2.9-.3-4.2-.8c-1.2-.6-2.3-1.3-3.1-2.3-.9-1-1.5-2.1-2-3.4-.4-1.3-.7-2.7-.7-4.2s.2-2.9.7-4.2c.4-1.3 1.1-2.4 2-3.4.9-1 1.9-1.7 3.1-2.3 1.2-.5 2.6-.8 4.2-.8zm0 17.6c.9 0 1.7-.2 2.4-.5s1.3-.8 1.7-1.4c.5-.6.8-1.3 1.1-2.2.2-.8.4-1.7.4-2.7v-.1c0-1-.1-1.9-.4-2.7-.2-.8-.6-1.6-1.1-2.2-.5-.6-1.1-1.1-1.7-1.4-.7-.3-1.5-.5-2.4-.5s-1.7.2-2.4.5-1.3.8-1.7 1.4c-.5.6-.8 1.3-1.1 2.2-.2.8-.4 1.7-.4 2.7v.1c0 1 .1 1.9.4 2.7.2.8.6 1.6 1.1 2.2.5.6 1.1 1.1 1.7 1.4.6.3 1.4.5 2.4.5zM88.9 28 83 7h4.7l4 15.7h.1l4-15.7h4.7l-5.9 21h-5.7zm28.8-3.6c.6 0 1.2-.1 1.7-.3.5-.2 1-.4 1.4-.8.4-.4.7-.8.9-1.4.2-.6.3-1.2.3-2v-13h4.1v13.6c0 1.2-.2 2.2-.6 3.1s-1 1.7-1.8 2.4c-.7.7-1.6 1.2-2.7 1.5-1 .4-2.2.5-3.4.5-1.2 0-2.4-.2-3.4-.5-1-.4-1.9-.9-2.7-1.5-.8-.7-1.3-1.5-1.8-2.4-.4-.9-.6-2-.6-3.1V6.9h4.2v13c0 .8.1 1.4.3 2 .2.6.5 1 .9 1.4.4.4.8.6 1.4.8.6.2 1.1.3 1.8.3zm13-17.4h4.2v9.1l7.4-9.1h5.2l-7.2 8.4L148 28h-4.9l-5.5-9.4-2.7 3V28h-4.2V7zm-27.6 16.1c-1.5 0-2.7 1.2-2.7 2.7s1.2 2.7 2.7 2.7 2.7-1.2 2.7-2.7-1.2-2.7-2.7-2.7z"></path>
                        </svg>
                    </fo:instream-foreign-object>
                </fo:block>
                <fo:block role="H1" font-size="16pt" font-weight="bold" margin-bottom="1mm">@messages("service.name")</fo:block>
            </fo:block>
        </fo:static-content>

        <!-- Body -->
        <fo:flow flow-name="xsl-region-body">

            <fo:block-container margin-bottom="10mm">

                <!-- cya heading -->
                <fo:block role="H2" id="subscription-caption" color="#505a5f" font-size="18pt" margin-top="2mm">@messages("checkYourAnswers.heading.caption")</fo:block>
                <fo:block role="H1" id="subscription-title" font-size="24pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.heading")</fo:block>

                <!-- ultimate parent entity details-->
                <fo:block role="H2" id="upe-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.upe")</fo:block>

                <fo:block-container margin-bottom="10mm">
                    @if(upeModel.upeRegisteredInUK) {
                        @upeModel.upeEntityType.map { upeEntityType =>
                            @if(upeEntityType == EntityType.UkLimitedCompany) {
                                <!-- entityTypeIncorporatedCompanyName -->
                                @upeModel.entityTypeIncorporatedCompanyName.map { entityTypeIncorporatedCompanyName =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyName</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypeIncorporatedCompanyReg -->
                                @upeModel.entityTypeIncorporatedCompanyReg.map { entityTypeIncorporatedCompanyReg =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyReg</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypeIncorporatedCompanyUtr -->
                                @upeModel.entityTypeIncorporatedCompanyUtr.map { entityTypeIncorporatedCompanyUtr =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyUtr</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                            } else {
                                <!-- entityTypePartnershipCompanyName -->
                                @upeModel.entityTypePartnershipCompanyName.map { entityTypePartnershipCompanyName =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypePartnershipCompanyName</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypePartnershipCompanyReg -->
                                @upeModel.entityTypePartnershipCompanyReg.map { entityTypePartnershipCompanyReg =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypePartnershipCompanyReg</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypePartnershipCompanyUtr -->
                                @upeModel.entityTypePartnershipCompanyUtr.map { entityTypePartnershipCompanyUtr =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>entityTypePartnershipCompanyUtr</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                            }
                        }
                    } else {
                        <!-- upeNameRegistration -->
                        @upeModel.upeNameRegistration.map { upeNameRegistration =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upeNameRegistration.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeNameRegistration</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeRegisteredAddress -->
                        @upeModel.upeRegisteredAddress.map { upeRegisteredAddress =>
                            <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                <fo:list-item  margin-bottom="3mm">
                                    <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upeRegisteredAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                    <fo:list-item-body start-indent="body-start()">
                                        @upeRegisteredAddress.getAddressList(countryOptions).map { line =>
                                        <fo:block>@line</fo:block>
                                        }
                                    </fo:list-item-body>
                                </fo:list-item>
                            </fo:list-block>
                        }

                        <!-- upeContactName -->
                        @upeModel.upeContactName.map { upeContactName =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upe-input-business-name.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeContactName</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeContactEmail -->
                        @upeModel.upeContactEmail.map { upeContactEmail =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upe-input-business-email.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeContactEmail</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upePhonePreference -->
                        @upeModel.upePhonePreference.map { upePhonePreference =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactUPEByTelephone.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@if(upePhonePreference){Yes}else{No}</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeCapturePhone -->
                        @upeModel.upeCapturePhone.map { upeCapturePhone =>
                            <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                <fo:list-item  margin-bottom="3mm">
                                    <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("captureTelephoneDetails.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                    <fo:list-item-body start-indent="body-start()">
                                        <fo:block>@upeCapturePhone</fo:block>
                                    </fo:list-item-body>
                                </fo:list-item>
                            </fo:list-block>
                        }
                    }
                </fo:block-container>

                <!-- nominated filing member details-->
                <fo:block role="H2" id="fm-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.nfm")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- fmYesNo -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("NominateFilingMemberYesNo.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@if(fmModel.fmYesNo){Yes}else{No}</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    @if(fmModel.fmYesNo) {
                        @fmModel.fmRegisteredInUK.map { fmRegisteredInUK =>
                            @if(fmRegisteredInUK) {
                                @fmModel.fmEntityType.map { fmEntityType =>
                                    @if(fmEntityType == EntityType.UkLimitedCompany) {
                                        <!-- fmEntityTypeIncorporatedCompanyName -->
                                        @fmModel.fmEntityTypeIncorporatedCompanyName.map { fmEntityTypeIncorporatedCompanyName =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypeIncorporatedCompanyName</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                        <!-- fmEntityTypeIncorporatedCompanyReg -->
                                        @fmModel.fmEntityTypeIncorporatedCompanyReg.map { fmEntityTypeIncorporatedCompanyReg =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
