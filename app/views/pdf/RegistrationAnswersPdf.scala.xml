@import models.subscription.{upeJourney, fmJourney, groupJourney, contactJourney}
@import models.grs.EntityType
@import utils.countryOptions.CountryOptions

@this()

@(upeModel: upeJourney, fmModel: fmJourney, groupModel: groupJourney, contactModel: contactJourney, countryOptions: CountryOptions)(implicit request: RequestHeader, messages: Messages)

<fo:root xml:lang="@messages.lang.locale.getLanguage" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:fox="http://xmlgraphics.apache.org/fop/extensions" font-family="sans-serif">

    <fo:layout-master-set>
        <fo:simple-page-master master-name="main" page-height="297mm" page-width="210mm" margin="2cm">
            <fo:region-body region-name="xsl-region-body" margin-top="3cm"/>
            <fo:region-before region-name="xsl-region-before"/>
            <fo:region-after region-name="xsl-region-after"/>
        </fo:simple-page-master>
    </fo:layout-master-set>

    <fo:declarations>
        <pdf:catalog xmlns:pdf="http://xmlgraphics.apache.org/fop/extensions/pdf">
            <pdf:dictionary type="normal" key="ViewerPreferences">
                <pdf:boolean key="DisplayDocTitle">true</pdf:boolean>
            </pdf:dictionary>
        </pdf:catalog>
    </fo:declarations>

    <fo:bookmark-tree>
        <fo:bookmark internal-destination="title">
            <fo:bookmark-title>@messages("service.name")</fo:bookmark-title>

            <fo:bookmark internal-destination="filing-member-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.filingmember")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="primary-contact-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.first")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="secondary-contact-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.second")</fo:bookmark-title>
            </fo:bookmark>

            <fo:bookmark internal-destination="contact-address-details">
                <fo:bookmark-title>@messages("rfm.contactCheckYourAnswers.address")</fo:bookmark-title>
            </fo:bookmark>

        </fo:bookmark>
    </fo:bookmark-tree>

    <fo:page-sequence master-reference="main">

        <!-- Header -->
        <fo:static-content flow-name="xsl-region-before">
            <fo:block border-color="#1d70b8" border-after-style="solid" border-after-width="5pt">
                <fo:external-graphic content-width="148px" content-height="20px"
                                     src="url(conf/resources/gov-uk-logo.png)" padding-right="1cm"
                                     fox:alt-text="GOV.UK"/>
                <fo:block role="H1" font-size="16pt" font-weight="bold" margin-bottom="1mm">@messages("service.name")
                </fo:block>
            </fo:block>
        </fo:static-content>

        <!-- Body -->
        <fo:flow flow-name="xsl-region-body">

            <fo:block-container margin-bottom="10mm">

                <!-- cya heading -->
                <fo:block role="H2" id="subscription-caption" color="#505a5f" font-size="18pt" margin-top="2mm">@messages("checkYourAnswers.heading.caption")</fo:block>
                <fo:block role="H1" id="subscription-title" font-size="24pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.heading")</fo:block>

                <!-- Ultimate Parent Entity details-->
                <fo:block role="H2" id="upe-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.upe")</fo:block>

                <fo:block-container margin-bottom="10mm">
                    @if(upeModel.upeRegisteredInUK) {
                        @upeModel.upeEntityType.map { upeEntityType =>
                            @if(upeEntityType == EntityType.UkLimitedCompany) {
                                <!-- entityTypeIncorporatedCompanyName -->
                                @upeModel.entityTypeIncorporatedCompanyName.map { entityTypeIncorporatedCompanyName =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyName</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypeIncorporatedCompanyReg -->
                                @upeModel.entityTypeIncorporatedCompanyReg.map { entityTypeIncorporatedCompanyReg =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyReg</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypeIncorporatedCompanyUtr -->
                                @upeModel.entityTypeIncorporatedCompanyUtr.map { entityTypeIncorporatedCompanyUtr =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypeIncorporatedCompanyUtr</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                            } else {
                                <!-- entityTypePartnershipCompanyName -->
                                @upeModel.entityTypePartnershipCompanyName.map { entityTypePartnershipCompanyName =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypePartnershipCompanyName</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypePartnershipCompanyReg -->
                                @upeModel.entityTypePartnershipCompanyReg.map { entityTypePartnershipCompanyReg =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@entityTypePartnershipCompanyReg</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                                <!-- entityTypePartnershipCompanyUtr -->
                                @upeModel.entityTypePartnershipCompanyUtr.map { entityTypePartnershipCompanyUtr =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>entityTypePartnershipCompanyUtr</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }
                            }
                        }
                    } else {
                        <!-- upeNameRegistration -->
                        @upeModel.upeNameRegistration.map { upeNameRegistration =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upeNameRegistration.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeNameRegistration</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeRegisteredAddress -->
                        @upeModel.upeRegisteredAddress.map { upeRegisteredAddress =>
                            <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                <fo:list-item  margin-bottom="3mm">
                                    <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upeRegisteredAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                    <fo:list-item-body start-indent="body-start()">
                                        @upeRegisteredAddress.getAddressList(countryOptions).map { line =>
                                        <fo:block>@line</fo:block>
                                        }
                                    </fo:list-item-body>
                                </fo:list-item>
                            </fo:list-block>
                        }

                        <!-- upeContactName -->
                        @upeModel.upeContactName.map { upeContactName =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upe-input-business-name.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeContactName</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeContactEmail -->
                        @upeModel.upeContactEmail.map { upeContactEmail =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("upe-input-business-email.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@upeContactEmail</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upePhonePreference -->
                        @upeModel.upePhonePreference.map { upePhonePreference =>
                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                            <fo:list-item  margin-bottom="3mm">
                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactUPEByTelephone.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                <fo:list-item-body start-indent="body-start()">
                                    <fo:block>@if(upePhonePreference){Yes}else{No}</fo:block>
                                </fo:list-item-body>
                            </fo:list-item>
                        </fo:list-block>
                        }

                        <!-- upeCapturePhone -->
                        @upeModel.upeCapturePhone.map { upeCapturePhone =>
                            <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                <fo:list-item  margin-bottom="3mm">
                                    <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("captureTelephoneDetails.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                    <fo:list-item-body start-indent="body-start()">
                                        <fo:block>@upeCapturePhone</fo:block>
                                    </fo:list-item-body>
                                </fo:list-item>
                            </fo:list-block>
                        }
                    }
                </fo:block-container>

                <!-- nominated filing member details-->
                <fo:block role="H2" id="fm-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.nfm")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- fmYesNo -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("NominateFilingMemberYesNo.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@if(fmModel.fmYesNo){Yes}else{No}</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    @if(fmModel.fmYesNo) {
                        @fmModel.fmRegisteredInUK.map { fmRegisteredInUK =>
                            @if(fmRegisteredInUK) {
                                @fmModel.fmEntityType.map { fmEntityType =>
                                    @if(fmEntityType == EntityType.UkLimitedCompany) {
                                        <!-- fmEntityTypeIncorporatedCompanyName -->
                                        @fmModel.fmEntityTypeIncorporatedCompanyName.map { fmEntityTypeIncorporatedCompanyName =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypeIncorporatedCompanyName</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                        <!-- fmEntityTypeIncorporatedCompanyReg -->
                                        @fmModel.fmEntityTypeIncorporatedCompanyReg.map { fmEntityTypeIncorporatedCompanyReg =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypeIncorporatedCompanyReg</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                        <!-- fmEntityTypeIncorporatedCompanyUtr -->
                                        @fmModel.fmEntityTypeIncorporatedCompanyUtr.map { fmEntityTypeIncorporatedCompanyUtr =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypeIncorporatedCompanyUtr</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                    } else {
                                        <!-- fmEntityTypePartnershipCompanyName -->
                                        @fmModel.fmEntityTypePartnershipCompanyName.map { fmEntityTypePartnershipCompanyName =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypePartnershipCompanyName</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                        <!-- fmEntityTypePartnershipCompanyReg -->
                                        @fmModel.fmEntityTypePartnershipCompanyReg.map { fmEntityTypePartnershipCompanyReg =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyReg.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypePartnershipCompanyReg</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                        <!-- fmEntityTypePartnershipCompanyUtr -->
                                        @fmModel.fmEntityTypePartnershipCompanyUtr.map { fmEntityTypePartnershipCompanyUtr =>
                                        <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                            <fo:list-item  margin-bottom="3mm">
                                                <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("entityType.companyUtr.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                                <fo:list-item-body start-indent="body-start()">
                                                    <fo:block>@fmEntityTypePartnershipCompanyUtr</fo:block>
                                                </fo:list-item-body>
                                            </fo:list-item>
                                        </fo:list-block>
                                        }
                                    }
                                }
                            } else {
                                <!-- fmNameRegistration -->
                                @fmModel.fmNameRegistration.map { fmNameRegistration =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("nfmNameRegistration.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@fmNameRegistration</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }

                                <!-- fmRegisteredAddress -->
                                @fmModel.fmRegisteredAddress.map { fmRegisteredAddress =>
                                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                        <fo:list-item  margin-bottom="3mm">
                                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("nfmRegisteredAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                            <fo:list-item-body start-indent="body-start()">
                                                @fmRegisteredAddress.getAddressList(countryOptions).map { line =>
                                                    <fo:block>@line</fo:block>
                                                }
                                            </fo:list-item-body>
                                        </fo:list-item>
                                    </fo:list-block>
                                }

                                <!-- fmContactName -->
                                @fmModel.fmContactName.map { fmContactName =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("nfmContactName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@fmContactName</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }

                                <!-- fmEmailAddress -->
                                @fmModel.fmEmailAddress.map { fmEmailAddress =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("nfmEmailAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@fmEmailAddress</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }

                                <!-- fmTelephonePreference -->
                                @fmModel.fmTelephonePreference.map { fmTelephonePreference =>
                                <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                    <fo:list-item  margin-bottom="3mm">
                                        <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactNfmByTelephone.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                        <fo:list-item-body start-indent="body-start()">
                                            <fo:block>@if(fmTelephonePreference){Yes}else{No}</fo:block>
                                        </fo:list-item-body>
                                    </fo:list-item>
                                </fo:list-block>
                                }

                                <!-- upeCapturePhone -->
                                @fmModel.fmContactTelephone.map { fmContactTelephone =>
                                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                                        <fo:list-item  margin-bottom="3mm">
                                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("nfmCaptureTelephoneDetails.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                                            <fo:list-item-body start-indent="body-start()">
                                                <fo:block>@fmContactTelephone</fo:block>
                                            </fo:list-item-body>
                                        </fo:list-item>
                                    </fo:list-block>
                                }
                            }
                        }
                    }
                </fo:block-container>


                <!-- group details-->
                <fo:block role="H2" id="group-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("checkYourAnswers.furtherRegistrationDetail")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- mneOrDomestic -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("mneOrDomestic.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@messages(s"mneOrDomestic.${groupModel.mneOrDomestic}")</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    <!-- groupAccountingPeriod -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("groupAccountingPeriod.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()"><fo:block> </fo:block></fo:list-item-body>
                        </fo:list-item>
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("groupAccountingStartDatePeriod.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@groupModel.groupAccountingPeriodStartDate</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("groupAccountingEndDatePeriod.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@groupModel.groupAccountingPeriodEndDate</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                </fo:block-container>


                <!-- Primary Contact Details -->
                <fo:block role="H2" id="primary-contact-details" font-size="18pt" font-weight="bold" margin-bottom="5mm" margin-top="7mm">@messages("checkYourAnswers.first")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- Primary contact name -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactNameCompliance.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@contactModel.primaryContactName</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    <!-- Primary contact email -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactEmailAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@contactModel.primaryContactEmail</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    <!-- Contact by telephone ? -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactByTelephone.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@if(contactModel.primaryContactByTelephone){Yes}else{No}</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                    <!-- Telephone number -->
                    @contactModel.primaryContactTelephone.map { primaryContactTelephone =>
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("contactCaptureTelephoneDetails.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@primaryContactTelephone</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>
                    }

                </fo:block-container>

                <!-- Secondary Contact Details -->
                <fo:block role="H2" id="secondary-contact-details" font-size="18pt" font-weight="bold" margin-bottom="5mm" margin-top="7mm">@messages("contactCheckYourAnswers.second")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- Secondary contact ? -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("addSecondaryContact.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@if(contactModel.secondaryContact){Yes}else{No}</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>


                    @if(contactModel.secondaryContact) {

                    <!-- Secondary contact name -->
                    @contactModel.secondaryContactName.map { secondaryContactName =>
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("secondaryContactName.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@secondaryContactName</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>
                    }

                    <!-- Secondary contact email -->
                    @contactModel.secondaryContactEmail.map { secondaryContactEmail =>
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("secondaryContactEmail.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@secondaryContactEmail</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>
                    }

                    <!-- Secondary contact by telephone ? -->
                    @contactModel.secondaryContactByTelephone.map { secondaryContactByTelephone =>
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("secondaryTelephonePreference.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@if(secondaryContactByTelephone){Yes}else{No}</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>
                    }

                    @contactModel.secondaryContactTelephone.map { secondaryContactTelephone =>
                    <!-- Secondary contact telephone number -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("secondaryTelephone.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                <fo:block>@secondaryContactTelephone</fo:block>
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>
                    }

                    }

                </fo:block-container>

                <!-- Contact address details -->
                <fo:block role="H2" id="contact-address-details" font-size="18pt" font-weight="bold" margin-bottom="5mm">@messages("contactCheckYourAnswers.address")</fo:block>

                <fo:block-container margin-bottom="10mm">

                    <!-- Contact Address -->
                    <fo:list-block margin-bottom="3mm" border-color="#b1b4b6" border-after-style="solid" border-after-width="0.5pt" provisional-label-separation="1em" provisional-distance-between-starts="14em">
                        <fo:list-item  margin-bottom="3mm">
                            <fo:list-item-label end-indent="label-end()" font-weight="bold"><fo:block>@messages("subscriptionAddress.checkYourAnswersLabel")</fo:block></fo:list-item-label>
                            <fo:list-item-body start-indent="body-start()">
                                @contactModel.contactAddress.getAddressList(countryOptions).map { line =>
                                <fo:block>@line</fo:block>
                                }
                            </fo:list-item-body>
                        </fo:list-item>
                    </fo:list-block>

                </fo:block-container>


            </fo:block-container>

        </fo:flow>

    </fo:page-sequence>

</fo:root>